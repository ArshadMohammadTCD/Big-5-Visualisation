<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns="http://www.tableau.com/xml/extension_manifest" manifest-version="0.1">
  <worksheet-extension id="com.appsfortableau.extensions.show-me-more-share" extension-version="0.1.0">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description>Radar Chart</description>
    <author name="Infotopics | AppsforTableau" email="support@appsfortableau.com" organization="Infotopics | AppsforTableau" website="https://appsfortableau.infotopics.com"/>
    <min-api-version>0.9</min-api-version>
    <source-location>
      <url>https://extensions-appsfortableau.infotopics.com/login/fMGbqwGsmLvZs3czC7HNqRth9m4PX8PT%2F2Rkp6Su%2BrbjhSQ28g9BBQjQYzkRriLL2x5uQvIrfyy50q0ek45b23%2BPxxH6BMJxyayoVDR3gwRVt9I%2BNFQ%2BYl7RgVhI6T6ZKjdcrk%2BMc9yoguNdGAIMBA%3D%3D/smm-radar/</url>
    </source-location>
    <icon></icon>
    <context-menu>
      <configure-context-menu-item></configure-context-menu-item>
    </context-menu>
    <encoding id="smm-radar+dimensions">
      <display-name>Dimensions</display-name>
      <data-spec>
        <data-type>string</data-type>
      </data-spec>
      <encoding-icon token="level"/>
      <tooltip>Add dimensions to required to create radar chart</tooltip>
    </encoding>
    <encoding id="smm-radar+xAxis">
      <display-name>Spokes</display-name>
      <data-spec>
        <data-type>string</data-type>
      </data-spec>
      <fields max-count="1"/>
      <encoding-icon token="delta-up"/>
      <tooltip>X-Axis value</tooltip>
    </encoding>
    <encoding id="smm-radar+measures">
      <display-name>Measure</display-name>
      <data-spec>
        <data-type>numeric</data-type>
      </data-spec>
      <fields max-count="1"/>
      <encoding-icon token="metric"/>
      <tooltip>Value</tooltip>
    </encoding>
  </worksheet-extension>
  <resources>
    <resource id="name">
      <text locale="en_US">Radar Chart</text>
    </resource>
  </resources>
</manifest>
